{
  "name": "react-mesto-auth",
  "version": "0.1.0",
  "description": "An interactive page for posting and liking photos. And customizing the user, of course.",
  "homepage": "https://artginzburg.github.io/react-mesto-auth/",
  "author": "Art Ginzburg",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp build/index.html build/404.html",
